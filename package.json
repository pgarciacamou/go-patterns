{
  "name": "go-patterns",
  "version": "1.0.2",
  "description": "JavaScript Design Pattern Generator",
  "main": "dist/patterns.module.js",
  "scripts": {
    "build": "cp -a ./node_modules/pipeline/src/. ./vendor/pipeline/",
    "prepush": "npm run lint",
    "precommit": "npm run lint",
    "compile": "npm run build && ./node_modules/browserify/bin/cmd.js index.js -o dist/go-patterns.js -t [ babelify --presets [ es2015 ] ] -s patterns",
    "dist": "mkdir -p dist && npm run compile && npm run uglify",
    "dist2": "./build.sh",
    "uglify": "./node_modules/uglify-js/bin/uglifyjs --compress -o dist/go-patterns.min.js -- dist/go-patterns.js",
    "test": "npm run build && ./node_modules/karma/bin/karma start",
    "lint": "./node_modules/.bin/eslint src/** specs/**",
    "lintfix": "./node_modules/.bin/eslint src/** specs/** --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pgarciacamou/go-patterns.git"
  },
  "keywords": [
    "design",
    "go-patterns",
    "pattern",
    "patterns",
    "generator",
    "patternity",
    "patternify",
    "protopattern",
    "patternizer"
  ],
  "author": "Pablo Garcia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pgarciacamou/go-patterns/issues"
  },
  "homepage": "https://github.com/pgarciacamou/go-patterns#readme",
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "babel-preset-es2015": "^6.13.2",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "eslint": "^3.18.0",
    "eslint-plugin-async": "^0.1.1",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.30.4",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-browserify": "^5.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "phantomjs-prebuilt": "^2.1.7",
    "uglify-js": "^2.8.21",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "pipe": "git+https://github.com/pgarciacamou/pipeline.git#master",
    "promise-polyfill": "6.0.1"
  }
}
